{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node2",
        "request": "launch",
        "name": "Launch server",
        "program": "${workspaceRoot}/server/src/main.ts",
        "cwd": "${workspaceRoot}",
        "sourceMaps": true,
        "outFiles": [
          "${workspaceRoot}/js/server/dist/main.js"
        ]
      },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "outFiles": []
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858,
            "outFiles": []
        }
    ]
}
